{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/e_Comerce_service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/e_Comerce_service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/e_Comerce_service",
  "projects": {
    "e_Comerce_service": {
      "type": "application",
      "root": "apps/e_Comerce_service",
      "entryFile": "main",
      "sourceRoot": "apps/e_Comerce_service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/e_Comerce_service/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    },
    "category": {
      "type": "application",
      "root": "apps/category",
      "entryFile": "main",
      "sourceRoot": "apps/category/src",
      "compilerOptions": {
        "tsConfigPath": "apps/category/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    },
    "brand": {
      "type": "application",
      "root": "apps/brand",
      "entryFile": "main",
      "sourceRoot": "apps/brand/src",
      "compilerOptions": {
        "tsConfigPath": "apps/brand/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    },
    "order-item": {
      "type": "application",
      "root": "apps/order-item",
      "entryFile": "main",
      "sourceRoot": "apps/order-item/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-item/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    }
  }
}